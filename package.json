{
  "name": "@jamessawle/java-lambda",
  "version": "0.0.0",
  "description": "Functional programming library bring Java Optional and lazily evaluated Streams to Javascript and Typescript",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "James Sawle"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "typescript",
    "functional",
    "Optional",
    "Stream",
    "Java"
  ],
  "scripts": {
    "lint": "pnpm tsc && pnpm eslint . && pnpm prettier --check .",
    "lint:fix": "pnpm eslint . --fix && pnpm prettier --write .",
    "build": "pnpm tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "pnpm tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "find ./src -name '*.test.ts' -exec node --import tsx --test {} \\;",
    "test:watch": "find ./src -name '*.test.ts' -exec node --import tsx --test --watch {} \\;",
    "playground": "pnpm build && node playground/index.mjs",
    "playground:test": "pnpm build && find ./playground -name '*.test.ts' -exec node --import tsx --test {} \\;"
  },
  "packageManager": "pnpm@8.15.4+sha256.cea6d0bdf2de3a0549582da3983c70c92ffc577ff4410cbf190817ddc35137c2",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.1.0"
  }
}
